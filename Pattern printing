import java.util.*;
import java.lang.*;
import java.io.*;

class Pattern
{
	public static void main (String[] args)
	{
		Scanner sc= new Scanner(System.in); //System.in is a standard input stream.
		int inp= sc.nextInt();
		int count=inp; 
		int min=0;
		for (int i = 1; i <= inp; i++)  
	    { 
	        for (int j = 1; j <= inp; j++)  
	        { 
	            min = i < j ? i : j; 
	            System.out.print(inp - min + 1); 
	        } 
	        System.out.println(); 
	    } 
	}
}
